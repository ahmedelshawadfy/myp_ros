#!/usr/bin/env python

from myp_ros.srv import *
import rospy

def handle(req):
	print "Returning just error message"
	return AddTwoIntsResponse(req.a + req.b)

def add_two_ints_server():
	rospy.init_node('server_test')
	rospy.Service('send_sensor_instruction', SendSensorInstruction, handle)
	rospy.spin()

if __name__ == "__main__":
	add_two_ints_server()
